package HearingTest;

import javax.swing.JFrame;

public class HearingTest{

    public static void main(String[] args) 
    {
        HTJFrame hearingJFrame = new HTJFrame();
        hearingJFrame.setDefaultCloseOperation ( JFrame.DO_NOTHING_ON_CLOSE );
        hearingJFrame.setSize( 500, 150);
        hearingJFrame.setVisible( true );
        hearingJFrame.setLocationRelativeTo(null);
    }
}
