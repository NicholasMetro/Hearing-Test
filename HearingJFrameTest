package HearingTestJFrame;

import javax.swing.JFrame;

public class HearingJFrameTest {

    public static void main(String[] args) 
    {
        HTJFrame hearingJFrame = new HTJFrame();
        hearingJFrame.setDefaultCloseOperation ( JFrame.EXIT_ON_CLOSE );
        hearingJFrame.setSize( 500, 250);
        hearingJFrame.setVisible( true );
        hearingJFrame.setLocationRelativeTo(null);
    }
    
}
