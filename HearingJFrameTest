package hearingjframetest;

import javax.swing.JFrame;

public class HearingJFrameTest {
    static HTJFrame hearingJFrame = new HTJFrame();
    public static void main(String[] args) 
    {
        
        hearingJFrame.setDefaultCloseOperation(HTJFrame.DISPOSE_ON_CLOSE);
        hearingJFrame.setSize( 500, 150);
        hearingJFrame.setVisible( true );
        hearingJFrame.setLocationRelativeTo(null);
    }
    
}
